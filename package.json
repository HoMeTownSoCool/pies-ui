{
  "name": "pies-ui",
  "private": true,
  "version": "1.0.0",
  "description": "A Vue 3 Component Library.Uses TypeScript.",
  "main": "index.js",
  "scripts": {
    "play": "pnpm run -C play dev",
    "clear": "rimraf dist",
    "clear:cache": "rimraf node_modules/.cache/ rimraf node_modules/.vite",
    "clear:lib": "rimraf node_modules",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prettier": "prettier --write .",
    "lint:css": "stylelint 'packages/theme-chalk/src/**/*.scss' --fix --custom-syntax postcss-scss",
    "lint:ls": "ls-lint",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoMeTownSoCool/pies-ui.git"
  },
  "keywords": [
    "vue3",
    "pies",
    "ui",
    "library",
    "component",
    "js",
    "ts",
    "vue"
  ],
  "author": "HoMeTown <hometownsocool@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HoMeTownSoCool/pies-ui/issues"
  },
  "homepage": "https://github.com/HoMeTownSoCool/pies-ui#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.4",
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@pies-ui/components": "workspace:*",
    "@pies-ui/eslint-config": "workspace:^0.0.1",
    "@pies-ui/theme-chalk": "workspace:*",
    "@pies-ui/utils": "workspace:^1.0.0"
  }
}
